<templateSet group="user">
  <template name="auto" value="&quot;autoload&quot;: {&#10;        &quot;psr-4&quot;: {&#10;            &quot;App\\&quot;: &quot;src/&quot;&#10;        },&#10;        &quot;classmap&quot;: [&#10;            &quot;app/ContaoManagerPlugin.php&quot;&#10;        ]&#10;    }" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="php" value="php_errors on&#10;php_flag log_errors on&#10;php_value error_log &quot;/www/htdocs/w0193421/$PROJECT$/current/var/logs/php_errors.log&quot;&#10;php_value memory_limit 1G" description="Turn on PHP error logs (.htaccess)" toReformat="false" toShortenFQNames="true">
    <variable name="PROJECT" expression="" defaultValue="project_folder" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="ECMAScript6" value="false" />
      <option name="HTML" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="JSON" value="false" />
      <option name="OTHER" value="true" />
      <option name="PHP" value="false" />
      <option name="REQUEST" value="false" />
      <option name="SHELL_SCRIPT" value="false" />
      <option name="SQL" value="false" />
      <option name="Twig" value="false" />
      <option name="TypeScript" value="false" />
      <option name="Vue" value="false" />
      <option name="XML" value="false" />
    </context>
  </template>
  <template name="@b" value="@include breakpoint($QUERY$) {&#10;  $END$&#10;}" description="@include breakpoint()" toReformat="false" toShortenFQNames="true">
    <variable name="QUERY" expression="" defaultValue="&quot;medium&quot;" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
    </context>
  </template>
  <template name="fetchp" value="fetch('$ENDPOINT$', {&#10;  method: 'POST',&#10;  credentials: 'same-origin',&#10;  headers: {'Content-Type': 'application/json'},&#10;  body: JSON.stringify($DATA$)&#10;  })&#10;  .then(function (r) { return r.json() })&#10;  .then(function (data) {&#10;  $END$&#10;  })&#10;  .catch(function (error) {&#10;  console.error(error);&#10;  });" description="fetch() POST" toReformat="true" toShortenFQNames="true">
    <variable name="ENDPOINT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DATA" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="VUE_SCRIPT" value="true" />
    </context>
  </template>
  <template name="stripejs" value="&lt;script src=&quot;https://js.stripe.com/v3/&quot;&gt;&lt;/script&gt;" description="Stripe script" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TOP_LEVEL" value="true" />
    </context>
  </template>
</templateSet>